{
  "name": "ims-mui-dashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "prisma generate",
    "db:validate": "dotenv -e .env.local -- prisma validate",
    "db:status": "dotenv -e .env.local -- prisma migrate status",
    "db:dev": "dotenv -e .env.local -- prisma migrate dev",
    "db:deploy": "dotenv -e .env.local -- prisma migrate deploy",
    "db:reset": "dotenv -e .env.local -- prisma migrate reset --force",
    "db:studio": "dotenv -e .env.local -- prisma studio",
    "db:seed": "dotenv -e .env.local -- node prisma/seed.js"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@fortawesome/fontawesome-svg-core": "^6.5.2",
    "@fortawesome/free-regular-svg-icons": "^6.5.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@mui/icons-material": "^5.15.20",
    "@mui/material": "^5.15.20",
    "@mui/x-data-grid": "^7.7.0",
    "@mui/x-date-pickers": "^7.7.0",
    "@prisma/client": "^5.15.1",
    "@prisma/extension-accelerate": "^1.1.0",
    "@react-email/components": "^0.0.19",
    "@vercel/kv": "^1.0.1",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.4.5",
    "framer-motion": "^11.2.10",
    "html2canvas": "^1.4.1",
    "jspdf": "^2.5.1",
    "jspdf-autotable": "^3.8.2",
    "luxon": "^3.4.4",
    "next": "14.2.4",
    "next-auth": "^4.24.7",
    "react": "^18.3.1",
    "react-countup": "^6.5.3",
    "react-dom": "^18.3.1",
    "react-email": "2.1.4",
    "react-markdown": "^9.0.1",
    "recharts": "^2.12.7",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.0",
    "resend": "^3.2.0",
    "swr": "^2.2.5"
  },
  "devDependencies": {
    "dotenv-cli": "^7.4.2",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.4",
    "prisma": "^5.15.1"
  }
}
