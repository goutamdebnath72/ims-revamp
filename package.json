{
  "name": "ims-mui-dashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && prisma migrate deploy && next build",
    "build:local": "dotenv -e .env.local -- sh -c \"prisma generate --no-engine && prisma migrate deploy && next build\"",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "prisma generate --no-engine",
    "db:validate": "dotenv -e .env.local -- prisma validate",
    "db:status": "dotenv -e .env.local -- prisma migrate status",
    "db:dev": "dotenv -e .env.local -- prisma migrate dev",
    "db:deploy": "dotenv -e .env.local -- prisma migrate deploy",
    "db:reset": "dotenv -e .env.local -- prisma migrate reset --force",
    "db:studio": "dotenv -e .env.local -- prisma studio",
    "db:seed": "dotenv -e .env.local -- node prisma/seed.js",
    "db:seed:direct": "dotenv -e .env.local -- sh -c 'DATABASE_URL=$DIRECT_POSTGRES_URL node prisma/seed.js'"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^7.1.1",
    "@mui/material": "^7.1.1",
    "@mui/x-data-grid": "^8.5.2",
    "@mui/x-date-pickers": "^8.9.0",
    "@prisma/client": "^6.12.0",
    "@prisma/extension-accelerate": "^2.0.2",
    "@react-email/components": "^0.5.0",
    "@vercel/kv": "^3.0.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "dotenv": "^17.2.1",
    "framer-motion": "^12.18.1",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.1",
    "jspdf-autotable": "^5.0.2",
    "luxon": "^3.7.1",
    "next": "15.3.3",
    "next-auth": "^4.24.11",
    "react": "^19.0.0",
    "react-countup": "^6.5.3",
    "react-dom": "^19.0.0",
    "react-email": "^4.2.8",
    "react-markdown": "^10.1.0",
    "recharts": "^3.0.2",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.1",
    "resend": "^4.8.0",
    "swr": "^2.3.4"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.3.3",
    "prisma": "^6.12.0"
  }
}
